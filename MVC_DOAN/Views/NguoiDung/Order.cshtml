    <script>
        $('.nav-link').on('click', function (e) {
            e.preventDefault(); // Ngăn chặn hành động mặc định của tab link
            $(this).tab('show'); // Hiển thị tab-pane tương ứng với tab link được chọn
        });

        const notApprovedToggles = document.querySelectorAll('.not-approved-toggle');
        const approvedToggles = document.querySelectorAll('.approved-toggle');
        const deliveringToggles = document.querySelectorAll('.delivering-toggle');
        const deliveredToggles = document.querySelectorAll('.delivered-toggle');

        notApprovedToggles.forEach(toggle => {
            toggle.addEventListener('click', function () {
                // Tìm phần tử kế tiếp là 'not-approved-details'
                const details = this.nextElementSibling;

                // Kiểm tra hiển thị/ẩn thông tin
                if (details.style.display === 'none' || details.style.display === '') {
                    details.style.display = 'block'; // Hiển thị thông tin
                } else {
                    details.style.display = 'none'; // Ẩn thông tin
                }
            });
        });
